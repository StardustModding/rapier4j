class Triple<A, B, C> {
    bound A: Clone;
    bound B: Clone;
    bound C: Clone;

    [rust] field a: A;
    [rust] field b: B;
    [rust] field c: C;

    static init fn of(first: A, second: B, third: C) -> Triple<A, B, C>;
    
    fn first() -> A;
    fn second() -> B;
    fn third() -> C;
};
